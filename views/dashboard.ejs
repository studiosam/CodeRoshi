<!-- Wappler include head-page="layouts/main" fontawesome_5="cdn" bootstrap5="local" is="dmx-app" id="dashboard" appConnect="local" components="{dmxCalendar:{},dmxVideo:{}}" -->
<main>
    <section class="py-5">



        <div class="row">

            <div class="col p-5">

                <h3 class="text-end">Upcoming Sessions</h3><dmx-calendar id="calendar1" views="dayGridMonth,dayGridWeek,dayGridDay" theme="bootstrap5" hide-non-current-dates="true" now-indicator="true"></dmx-calendar>
            </div>
            <div class="col p-5 style9">
                <h3 class="text-end">Video Library</h3><video is="dmx-video" id="video1" src="https://www.youtube.com/watch?v=dQw4w9WgXcQ" preload="auto"></video>
                <video is="dmx-video" id="video2" src="https://www.youtube.com/watch?v=dQw4w9WgXcQ" preload="auto"></video>




            </div>


        </div>
        <div class="row">
            <div class="col p-5">

                <h3 class="text-end">Recent Projects</h3>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">First</th>
                                <th scope="col">Last</th>
                                <th scope="col">Handle</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>Mark</td>
                                <td>Otto</td>
                                <td>@mdo</td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td>Jacob</td>
                                <td>Thornton</td>
                                <td>@fat</td>
                            </tr>
                            <tr>
                                <th scope="row">3</th>
                                <td>Larry</td>
                                <td>the Bird</td>
                                <td>@twitter</td>
                            </tr>
                        </tbody>
                    </table>
                </div>

            </div>
            <div class="col p-5">
                <h3 class="text-end">Number of Sessions</h3>
                <div class="d-flex flex-row justify-content-end gap-4 align-items-center">
                    <p class="text-end fs-2">4</p><button id="btn1" class="btn bg-info style4">Book Session</button>
                </div>
                <div class="gap-3"></div>



                <div class="text-end"></div>
            </div>
        </div>
        <div class="row p-5">
            <div class="col">
                <h1>Referral Tracker</h1>
                <div class="progress">
                    <div class="progress-bar bg-success" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>

        </div>
    </section>
</main>
<meta name="ac:route" content="/dashboard">